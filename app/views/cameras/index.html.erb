<%= javascript_tag "var autocomplete_items = #{ @autocomplete_items.to_json };" %>

<script type="text/javascript">
    jQuery(document).ready(function() {
        $('#auto_complete').typeahead({name: 'model_name', local: autocomplete_items });
    });
</script>

<%=  form_tag('/cameras/search', method: :get) do %>
    <%= text_field_tag :auto_complete, nil, :placeholder => "Search for cameras" %>
    <%=  submit_tag 'submit' %>
 <% end %>
